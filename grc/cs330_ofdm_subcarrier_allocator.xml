<?xml version="1.0"?>
<block>
  <name>ofdm_subcarrier_allocator</name>
  <key>cs330_ofdm_subcarrier_allocator</key>
  <category>cs330</category>
  <import>import cs330</import>
  <make>cs330.ofdm_subcarrier_allocator()</make>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>48</vlen>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>64</vlen>
  </source>
</block>
